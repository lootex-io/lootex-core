  "scripts": {
    "prebuild": "rimraf dist",
    "build": "NODE_OPTIONS='--max-old-space-size=4096' nest build",
    "build:2gb": "NODE_OPTIONS='--max-old-space-size=2048' nest build",
    "build:low-memory": "NODE_OPTIONS='--max-old-space-size=1536' nest build",
    "build:1gb": "NODE_OPTIONS='--max-old-space-size=1024' nest build",
    "sentry:sourcemaps": "node -e \"require('./package.json'); process.env.SENTRY_RELEASE = require('./package.json').version; require('child_process').execSync('sentry-cli releases files $SENTRY_RELEASE upload-sourcemaps ./dist --ext js --ext map --url-prefix ~/dist --rewrite');\"",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "dev": "nest start --watch",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,test}/**/*.ts\" --fix",
    "test": "NODE_ENV=test jest --runInBand",
    "test:watch": "NODE_ENV=test jest --watch --runInBand",
    "test:cov": "NODE_ENV=test jest --coverage --runInBand",
    "test:debug": "NODE_ENV=test node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "NODE_ENV=test jest --config --runInBand ./test/jest-e2e.json",
    "queue": "nest start --config queue-cli.json",
    "update-opensea-collection": "node -r dotenv/config ./scripts/main.js",
    "currency-price": "nest start --config currency-price-cli.json",
    "order-sync": "nest start --config order-sync-cli.json",
    "task-paymaster": "nest start --config task-paymaster-cli.json",
    "task-aggregator": "nest start --config task-aggregator-cli.json",
    "event-poller": "nest start --config event-poller-cli.json",
    "event-poller-gp": "nest start --config event-poller-gp-cli.json",
    "version": "conventional-changelog -p angular -i CHANGELOG.md -s && git add CHANGELOG.md",
    "batch-owner-assets": "nest start --config batch-owner-assets-cli.json",
    "batch-asset-owners": "nest start --config batch-asset-owners-cli.json",
    "batch-contract-assets": "nest start --config batch-contract-assets-cli.json",
    "batch-asset-metadata": "nest start --config batch-asset-metadata-cli.json",
    "batch-collection-volume": "nest start --config batch-collection-volume-cli.json",
    "studio-ipfs": "nest start --config task-studio-ipfs-cli.json",
    "collection-trading-data": "nest start --config collection-trading-data-cli.json",
    "task-tx-tracking": "nest start --config task-tx-tracking-cli.json",
    "event-poller-nft-transfer": "nest start --config event-poller-nft-transfer-cli.json"
  },
